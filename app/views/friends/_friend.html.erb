<div id="<%= dom_id friend %>">
  <h2>
    <%= friend.first_name %>
    <%= friend.last_name %>
  </h2>
  <ul class="list-group list-group-flush">
    <li class="list-group-item">    
      <strong>Email:</strong>
      <%= friend.email %>
    </li>
    <li class="list-group-item">    
      <strong>Phone:</strong>
      <%= friend.phone %>
    </li>
    <li class="list-group-item">    
      <strong>Twitter:</strong>
      <%= friend.twitter %>
    </li>
    <li class="list-group-item">    
      <strong>User Id:</strong>
      <%= friend.user_id %>
    </li>
  </ul>
</div>
